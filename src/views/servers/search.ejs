<meta name="theme-color" content="#8c72da">
<meta property="og:image:alt" content="<%if(locals.url.split('filter=')[1]){%> <%=locals.language.search.filter.shorted%> <%=locals.url.split('filter=')[1]%> <%}else if(locals.url.split('premium=')[1]){%> <%=locals.language.search.filter.premium%><%}else if(locals.url.split('tags=')[1]){%> <%=locals.language.search.filter.tagged%> <%=locals.url.split('tags=')[1].split('&page=')[0]%> <%}%>" />
<meta property="og:site_name" content="Discolounge" />
<link rel='canonical' href='https://discolounge.net/' />

<meta property="twitter:url" content="https://discolounge.net/">
