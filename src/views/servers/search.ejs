<meta name="description" content="<%if(locals.url.split('filter=')[1]){%> <%=locals.language.search.filter.shorted%> <%=locals.url.split('filter=')[1]%> <%}else if(locals.url.split('premium=')[1]){%> <%=locals.language.search.filter.premium%><%}else if(locals.url.split('tags=')[1]){%> <%=locals.language.search.filter.tagged%> <%=locals.url.split('tags=')[1].split('&page=')[0]%> <%}%>">
