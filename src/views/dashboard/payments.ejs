    <div class="col-md-3">
      <%- include('../partials/vertical-navbar'); %>

    </div>
    <div class="col-md-9">

      <!-- Page content holder -->
<%if(data != undefined){%>
  <div id="content">

    <div class="form-content">
        <h1 style="padding:10px">History</h1>
        <div class="socials-content">

            <%data.payments.forEach(payment=>{%>
                <a  class="btn-show collapsed" data-toggle="collapse" data-target="#<%=payment.paymentId%>" aria-expanded="false" aria-controls="collapseTwo">
                    <i class="fas fa-caret-right"></i> (<%=payment.date%>)                 
                 </a>
              <div id="<%= payment.paymentId%>" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
                <div class="card-body">
                  <p class="help-block">- Plan: <%=payment.plan%> <br>
                    - Amount: $<%=payment.amount%> <br>
                    - Paypal Payment Id: <%=payment.paymentId%> </p>
                </div>
              </div>   
              <hr>             
              <%})%>
        </div>

      </div>
      

</div>
